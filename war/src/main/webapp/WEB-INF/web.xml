<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>TestWeb</display-name>
    <servlet>
        <description>servlets.RegistrationServlet</description>
        <display-name>servlets.RegistrationServlet</display-name>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>com.nc.j2ee.servlets.RegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>servlets.LoginServlet</description>
        <display-name>servlets.LoginServlet</display-name>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.nc.j2ee.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>servlets.LogoutServlet</description>
        <display-name>servlets.LogoutServlet</display-name>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.nc.j2ee.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>servlets.OrderServlet</description>
        <display-name>servlets.OrderServlet</display-name>
        <servlet-name>OrderServlet</servlet-name>
        <servlet-class>com.nc.j2ee.servlets.OrderServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderServlet</servlet-name>
        <url-pattern>/order</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>servlets.DeleteServlet</description>
        <display-name>servlets.DeleteServlet</display-name>
        <servlet-name>DeleteServlet</servlet-name>
        <servlet-class>com.nc.j2ee.servlets.DeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteServlet</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>servlets.EditServlet</description>
        <display-name>servlets.EditServlet</display-name>
        <servlet-name>EditServlet</servlet-name>
        <servlet-class>com.nc.j2ee.servlets.EditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditServlet</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>servlets.AddServlet</description>
        <display-name>servlets.AddServlet</display-name>
        <servlet-name>AddServlet</servlet-name>
        <servlet-class>com.nc.j2ee.servlets.AddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <ejb-local-ref>
        <ejb-ref-name>ejb/attributes</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.nc.j2ee.interfaces.TTAttributeInterface</local>
        <ejb-link>TTAttributesLocalSessionEJB</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>ejb/object</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.nc.j2ee.interfaces.TTObjectInterface</local>
        <ejb-link>TTObjectLocalSessionEJB</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>ejb/objectType</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.nc.j2ee.interfaces.TTObjectTypeInterface</local>
        <ejb-link>TTObjectTypeLocalSessionEJB</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>ejb/attrObjectType</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.nc.j2ee.interfaces.TTAttrObjectTypeInterface</local>
        <ejb-link>TTAttrObjectTypesLocalSessionEJB</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>ejb/params</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.nc.j2ee.interfaces.TTParamsInterface</local>
        <ejb-link>TTParamsLocalSessionEJB</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>ejb/references</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <local>com.nc.j2ee.interfaces.TTReferencesInterface</local>
        <ejb-link>TTReferencesLocalSessionEJB</ejb-link>
    </ejb-local-ref>

</web-app>